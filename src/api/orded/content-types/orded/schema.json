{
  "kind": "collectionType",
  "collectionName": "ordeds",
  "info": {
    "singularName": "orded",
    "pluralName": "ordeds",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "customerName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "products": {
      "type": "json"
    },
    "total": {
      "type": "decimal",
      "required": true
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "paid",
        "failed"
      ]
    },
    "stripeSessionId": {
      "type": "string"
    }
  }
}
